  var countObj = {"__CPS__26582811":0}; commentIndex = { elePrefix : 'dna-comment-count_', updateCommentCount : function(eleId, count) { var eleObj = document.getElementById(eleId); if (eleObj) { if (parseInt(count) > 0) { eleObj.innerHTML = count; } else { eleObj.innerHTML = '&nbsp;'; }; eleObj.className = eleObj.className + ' dna-comment-count-show'; } } }; for(var i in countObj) { var ele = commentIndex.elePrefix + i; commentIndex.updateCommentCount(ele, countObj[i]); }